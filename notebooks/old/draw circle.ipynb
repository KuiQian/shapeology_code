   "source": [
    "radius=15\n",
    "def circle_patch(radius):\n",
    "    size=2*radius+1\n",
    "    x=arange(-radius,radius+1)\n",
    "    xx=np.array([x for i in x])\n",
    "    yy=xx.T\n",
    "    d=sqrt(xx**2+yy**2)\n",
    "    return array((d<radius+0.1)*1,dtype=np.uint8)"
   ]
  },
